config rpcd
        option socket /var/run/ubus.sock
        option timeout 30

config login
        option username 'root'
        option password '$p$root'
        list read '*'
        list write '*

config login
         option username 'root'
         option password '$p$root'
         list read '*'
         list write '*'

config login
         option username 'frontend'
         option password 'test123'
         list read 'ubus'
         list write 'ubus'